{
  "name": "connectsm.art",
  "scope": "trezy-studios",
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@now/next"
    }
  ],
  "routes": [
    {
      "dest": "/storybook/index.html",
      "src": "/storybook/"
    },
    {
      "dest": "/storybook/$1",
      "src": "/storybook/(.*)"
    }
  ]
}
